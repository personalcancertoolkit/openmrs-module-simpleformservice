<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.simpleformservice">
    <class table="simpleformservice_data_access_permissions" name="DataAccessPermission">
        
        <id name="id" type="int" column="id"  unsaved-value="0">
            <generator class="native" />
        </id>
        
        <many-to-one name="grantedToPerson" class="org.openmrs.Person" not-null="true" column="granted_to_person_id" />

        <many-to-one name="accessToPerson" class="org.openmrs.Person" not-null="true" column="access_to_person_id" />

        <property name="encounterType" type="java.lang.String" column="encounter_type"/>

        <property name="permissionType" type="java.lang.String" column="permission_type"/>

    </class>
</hibernate-mapping>